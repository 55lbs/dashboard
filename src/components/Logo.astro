---
import Svg from './Svg.astro'

export type Props = {
  orientation?: 'horizontal' | 'vertical';
  color?: string;
};

const { orientation = 'horizontal', color = 'white' } = Astro.props as Props;

const logo = orientation === 'horizontal' ? 'logo.svg' : 'logo-vertical.svg';
---

<!-- <Fragment set:html={innerHTML} /> -->
 <Svg src={`../../public/assets/${logo}`} fill={color} />